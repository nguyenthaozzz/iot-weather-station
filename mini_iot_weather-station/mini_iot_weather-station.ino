void callback(char* topic, byte* payload, unsigned int length) {
  Serial.print("Message arrived on topic: ");
  Serial.print(topic);
  Serial.print(" | Message: ");
  String message = "";
  for (int i = 0; i < length; i++) {
    message += (char)payload[i];
  }
  Serial.println(message);

  if (String(topic) == commandTopic) {
    // ================= LCD Commands =================
    if (message == "OPEN") {
      Serial.println("üëâ Command: OPEN LCD");
      lcdWake();
      client.publish("iot/house/status", "LCD OPENED");
      return;
    } 
    if (message == "SLEEP") {
      Serial.println("üëâ Command: SLEEP LCD");
      lcdSleep();
      client.publish("iot/house/status", "LCD SLEPT");
      return;
    }

    // ================= City Change Command =================
    // N·∫øu message l√† JSON, v√≠ d·ª• {"cmd":"change_city","city":"Da Nang"}
    DynamicJsonDocument doc(256);
    DeserializationError error = deserializeJson(doc, message);
    if (!error) {
      String cmd = doc["cmd"];
      if (cmd == "change_city") {
        String newCity = doc["city"].as<String>();
        if (newCity.length() > 0) {
          city = newCity;
          Serial.println("üåç Nh·∫≠n l·ªánh ƒë·ªïi v·ªã tr√≠ sang: " + city);

          // C·∫≠p nh·∫≠t ngay d·ªØ li·ªáu th·ªùi ti·∫øt
          getWeather();

          // G·ª≠i ph·∫£n h·ªìi v·ªÅ server ƒë·ªÉ bi·∫øt ƒë√£ ƒë·ªïi
          client.publish("iot/house/status", ("CITY_CHANGED:" + city).c_str());
        }
      }
    } else {
      Serial.println("‚ö†Ô∏è L·ªánh kh√¥ng ph·∫£i JSON ho·∫∑c l·ªói parse!");
    }
  }
}



